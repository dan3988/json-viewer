{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Chrome",
			"type": "chrome",
			"userDataDir": "${workspaceFolder}\\.chrome",
			"request": "launch",
			"url": "chrome://extensions/",
			"runtimeArgs": ["--load-extension=${workspaceFolder}"]
		},
		{
			"name": "Firefox",
			"type": "firefox",
			"addonPath": "${workspaceFolder}",
			"url": "about:debugging#/runtime/this-firefox",
			"request": "launch"
		}
	]
}