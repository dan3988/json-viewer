{
	"$schema": "https://json.schemastore.org/chrome-manifest",
	"manifest_version": 2,
	"name": "Json Explorer",
	"version": "1.0.0",
	"content_scripts": [
		{
			"matches": [ "*://*/*" ],
			"js": [ "lib/content.js" ],
			"run_at": "document_end"
		}
	],
	"web_accessible_resources": [
		"lib/*.js",
		"res/*"
	],
	"permissions": [
		"tabs",
		"activeTab",
		"webNavigation"
	]
}